<%= form_for([:applicant, @profile], :html => { :class => 'standart_form' }) do |f| %>
	<div class='item'>
		<%= f.label 'Фамилия' %>
		<div class='field'>
			<%= f.text_field :lastname, :value => "#{@profile.lastname}" %>
		</div>
	</div>
	<div class='item'>
		<%= f.label 'Имя' %>
		<div class='field'>
			<%= f.text_field :firstname, :value => "#{@profile.firstname}" %>
		</div>
	</div>
	<div class='item'>
		<%= f.label 'Отчество' %>
		<div class='field'>
			<%= f.text_field :surename, :value => "#{@profile.surename}"%>
		</div>
	</div>
	<div class='item'>
		<%= f.label 'Дата рождения' %>
		<div class='field'>
			<%= f.text_field :date, :value => "#{@profile.date}" %>
		</div>
	</div>
	<div class='item' style="margin-bottom: 0px;">
		<%= f.label 'Мобильный телефон' %>
		<div class='field'>
			<%= f.text_field :phone, :value => "#{@profile.phone}" %><hr>
		</div>
	</div>
	<div class="buttons">
		<%= f.submit 'Сохранить', :class => 'btn btn-success' %>
	</div>
<% end %>
<img class="img-rounded" data-src="holder.js/140x140" alt="140x140" style="width: 140px; height: 140px;" src="data:image/png;base64,">