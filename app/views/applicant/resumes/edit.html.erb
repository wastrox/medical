<style>
	#profes-resume{
		margin: 15px 0 0 0;
	}
	/*input { width: 316px; }*/
	fieldset {
    	border: 1px dashed black;
    	margin: 10px;
    	padding: 20px;
  	}
.hint, .hint-pc {
		background-color: #F0F0F0;
		border-bottom-left-radius: 3px;
		border-bottom-right-radius: 3px;
		border-top-left-radius: 3px;
		border-top-right-radius: 3px;
		float: left;
		display: block;
		height: 20px;
		font-size: 14px;
		padding-bottom: 4px;
		padding-left: 0px;
		padding-right: 0px;
		margin: 0 15px 0 0;
		text-align: center;
		width: 100px;
	}
	.star, .star-pc {
		float: left;
		margin: 0 0 0 15px;	
	}
</style>
<a href="#">Резюме/Редактировать(хлебные крошки)</a>
<hr>
<div id="content-resume">
<%= nested_form_for([:applicant, @resume], :html => { :class => 'standart_form' } ) do |applicant_fields| %>
	<%= render :partial => 'resume', :locals => { :applicant => applicant_fields.object, :applicant_fields => applicant_fields} %>
<%= applicant_fields.submit %>
<% end %>
	<div class="clear"></div>
</div>




