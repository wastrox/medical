<% city = City.all %>

<ul id="triple">
	<% city.each do |city| %>
		<% cache city do %>
			<% city_name = city.singular("Р") %>
			<li><%= link_to city.name, city_vacancy_path(translit_helper(city.name)), title: "Сайт трудоустройства для жителей #{city_name}, медицинские вакансии." %></li>
		<% end %>
	<% end %>
</ul> 